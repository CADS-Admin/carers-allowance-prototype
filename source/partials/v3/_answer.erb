<%
if req.params["currentPage"].to_i > locals[:q]
%>

<tr>
    <td class="question"><%= locals[:q] %>.</td>
    <td class="title"><%= locals[:title] %></td>
    <td class="answer">
        <%= locals[:answer] %>
    </td>
    <td>
        <a href="<%= locals[:q] %>.html?edit=true&currentPage=<%= req.params["currentPage"] %>">Change this</a>
    </td>
</tr>

<%
end
%>